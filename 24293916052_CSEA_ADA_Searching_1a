#include <stdio.h>
#include <stdlib.h> 
#include <time.h>   

int linearSearch(int arr[], int n, int key) {
    for (int i = 0; i < n; i++) {
        if (arr[i] == key) {
            return i; 
        }
    }
    return -1; 
}

int main() {
    
    srand(time(NULL));

    
    int min_n = 10000;
    int max_n = 200000;
    int step = 10000;

    printf("n,time\n"); 

    for (int n = min_n; n <= max_n; n += step) {
        
        int *arr = (int *)malloc(n * sizeof(int));
        if (arr == NULL) {
            fprintf(stderr, "Memory allocation failed\n");
            return 1;
        }

        
        for (int i = 0; i < n; i++) {
            arr[i] = rand();
        }

        int key = -1;

        
        clock_t start, end;
        double cpu_time_used;

        start = clock();
        linearSearch(arr, n, key); 
        end = clock();

        cpu_time_used = ((double)(end - start)) / CLOCKS_PER_SEC;

        
        printf("%d,%f\n", n, cpu_time_used);

        
        free(arr);
    }

    return 0;
}

